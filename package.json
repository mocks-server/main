{
  "name": "@mocks-server/main",
  "version": "1.8.6",
  "description": "Mocks server supporting multiple api behaviors",
  "keywords": [
    "mocks",
    "server",
    "mock",
    "fixtures",
    "behaviors",
    "api",
    "rest",
    "cli",
    "express",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/main",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "mocks-server": "./bin/mocks-server"
  },
  "scripts": {
    "lint": "eslint *.js lib test",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:acceptance": "jest --config=jest.acceptance.config.js --runInBand",
    "test:coverage": "npm run test",
    "test:ci": "npm run test && npm run test:acceptance",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@mocks-server/core": "1.4.8",
    "@mocks-server/plugin-admin-api": "1.4.5",
    "@mocks-server/plugin-inquirer-cli": "1.3.5"
  },
  "devDependencies": {
    "coveralls": "3.0.9",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "fs-extra": "9.0.1",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.10",
    "lodash": "4.17.19",
    "prettier": "2.0.5",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "sinon": "9.0.2",
    "strip-ansi": "6.0.0",
    "tree-kill-sync": "1.0.0"
  },
  "lint-staged": {
    "lib/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x"
  }
}
