{
  "name": "@mocks-server/main",
  "version": "1.4.0",
  "description": "Mocks server supporting multiple api behaviors",
  "keywords": [
    "mocks",
    "server",
    "mock",
    "fixtures",
    "behaviors",
    "api",
    "rest",
    "cli",
    "express",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/main",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "mocks-server": "./bin/mocks-server"
  },
  "scripts": {
    "lint": "eslint index.js lib test jest.config.js jest.acceptance.config.js",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:acceptance": "jest --config=jest.acceptance.config.js --runInBand",
    "test:ci": "npm run test && npm run test:acceptance",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@mocks-server/core": "1.1.0",
    "@mocks-server/plugin-admin-api": "1.1.0",
    "@mocks-server/plugin-inquirer-cli": "1.1.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fs-extra": "^8.1.0",
    "husky": "3.0.9",
    "is-promise": "^2.1.0",
    "jest": "24.9.0",
    "lint-staged": "^9.4.2",
    "lodash": "4.17.15",
    "prettier": "^1.19.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "sinon": "^7.5.0",
    "strip-ansi": "6.0.0",
    "tree-kill-sync": "^1.0.0"
  },
  "lint-staged": {
    "lib/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "index.js": "eslint",
    "jest.config.js": "eslint",
    "jest.acceptance.config.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": ">8.0.0",
    "npm": ">6.0.0"
  }
}
