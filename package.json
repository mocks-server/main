{
  "name": "@mocks-server/main",
  "version": "2.4.0",
  "description": "Mock Server supporting multiple route variants and mocks",
  "keywords": [
    "mock",
    "server",
    "mocks",
    "route",
    "route variants",
    "api",
    "rest",
    "cli",
    "express",
    "middlewares",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/main",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "mocks-server": "./bin/mocks-server"
  },
  "scripts": {
    "lint": "eslint *.js lib test",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:e2e": "jest --config=jest.e2e.config.js --runInBand",
    "test:unit": "npm run test",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@mocks-server/core": "2.4.0",
    "@mocks-server/plugin-admin-api": "2.1.0",
    "@mocks-server/plugin-inquirer-cli": "2.1.0"
  },
  "devDependencies": {
    "cross-fetch": "3.1.4",
    "cross-spawn": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "fs-extra": "10.0.0",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "jest": "27.2.0",
    "lint-staged": "11.1.2",
    "lodash": "4.17.21",
    "prettier": "2.4.1",
    "sinon": "11.1.2",
    "strip-ansi": "6.0.0",
    "tree-kill": "1.2.2",
    "wait-on": "6.0.0"
  },
  "lint-staged": {
    "lib/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.js": "eslint"
  },
  "engines": {
    "node": "12.x || 14.x || 15.x || 16.x"
  }
}
