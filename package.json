{
  "name": "@mocks-server/main",
  "version": "1.8.3",
  "description": "Mocks server supporting multiple api behaviors",
  "keywords": [
    "mocks",
    "server",
    "mock",
    "fixtures",
    "behaviors",
    "api",
    "rest",
    "cli",
    "express",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/main",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "mocks-server": "./bin/mocks-server"
  },
  "scripts": {
    "lint": "eslint index.js lib test jest.config.js jest.acceptance.config.js",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:acceptance": "jest --config=jest.acceptance.config.js --runInBand",
    "test:coverage": "npm run test",
    "test:ci": "npm run test && npm run test:acceptance",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@mocks-server/core": "1.4.5",
    "@mocks-server/plugin-admin-api": "1.4.2",
    "@mocks-server/plugin-inquirer-cli": "1.3.2"
  },
  "devDependencies": {
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "fs-extra": "9.0.0",
    "husky": "4.2.3",
    "is-promise": "2.1.0",
    "jest": "25.2.4",
    "lint-staged": "10.1.3",
    "lodash": "4.17.15",
    "prettier": "2.0.2",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "sinon": "9.0.1",
    "strip-ansi": "6.0.0",
    "tree-kill-sync": "1.0.0"
  },
  "lint-staged": {
    "lib/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "index.js": "eslint",
    "jest.config.js": "eslint",
    "jest.acceptance.config.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "10.x || 12.x || 13.x"
  }
}
